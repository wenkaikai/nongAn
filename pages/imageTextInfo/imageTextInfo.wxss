/* pages/imageTextInfo/imageTextInfo.wxss */
page{
  background:#f8f8f8;
}
.top{
  text-align: center;
  margin-bottom:20rpx;
}
.enterpriseTitle{
  color:#4a4a4a;
  padding:20rpx 0;
  
}
.image{
  background:#fff;
  padding:40rpx 0;
}
.bottom{
  background:#fff;
  padding:0 20rpx 30rpx 20rpx;
}
.bottom view{
  display:flex;
  justify-content: space-between;
  color:#4a4a4a;
  padding:20rpx 0;
  align-items:center;
  border-bottom:1px solid #ccc;
  font-size:28rpx;
}
.bottom view:last-of-type{
  border-bottom:none;
}
.bottom view.title{
 justify-content: center;
 font-size:32rpx;
}
.bottom view text:first-child{
  white-space: nowrap;
  margin-right:20rpx;
}
.bottom view text:last-child{
  color:#9B9B9B;
}
